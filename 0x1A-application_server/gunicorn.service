[Unit]
Description=Gunicorn instance to serve AirBnb clone
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4
Environment="PATH=/home/ubuntu/env/flaskapp/bin"
ExecStart=gunicorn --workers 3 --bind 0.0.0.0:50003 web_dynamic.2-hbnb:app >> /tmp/airbnb-access.log 2> /tmp/airbnb-error.logirbnb-access.log

[Install]
WantedBy=multi-user.target


